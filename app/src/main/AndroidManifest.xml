<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hongik.alpha_money">

    <application
        android:name=".ApplicationSingleton"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- 팝업형식으로 추가되기 위해서 마지막에 .dialog 붙임-->
        <activity android:name=".Activity.AddDataActivity" android:theme="@android:style/Theme.DeviceDefault.Light.Dialog"></activity>
    </application>

</manifest>